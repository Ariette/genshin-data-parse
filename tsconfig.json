{
  "compilerOptions": {
    "target": "ES2020",
    "module": "esnext",
    "lib": ["ES2020"],
    "moduleResolution": "node",
    "baseUrl": "./lib/GenshinData",
    "allowSyntheticDefaultImports": true,
    "allowJs": true,
    "importHelpers": true,
    "noEmit": true,
    "resolveJsonModule": true,
    "alwaysStrict": true,
    "sourceMap": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": false,
    "noImplicitThis": false,
    "strictNullChecks": false
  },
  "include": [
    "src/**/*.ts",
    "src/prebuild.js"
  ],
  "exclude": [
    "node_modules",
    "**/*.json",
    "*.json"
  ]
}
