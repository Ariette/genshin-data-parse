{
  "name": "genshin-data-parser",
  "version": "1.0.0",
  "description": "parser for Dimbreath/GenshinData",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node --experimental-json-modules ./dist/index.js",
    "prebuild": "node ./src/prebuild.js",
    "build": "tsc -p ./src/tsconfig.json",
    "dev": "tsc -w -p ./src/tsconfig.json",
    "lint": "eslint . --ext .ts,.tsx"
  },
  "keywords": [
    "genshin"
  ],
  "author": "Ariette",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^16.0.0",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.3.2",
    "typescript": "^4.3.5"
  }
}
